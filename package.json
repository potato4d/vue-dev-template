{
  "name": "vue-dev-template",
  "version": "0.0.1",
  "description": "Vue.js develop template.",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run serve",
    "lint": "eslint ./src/ --ext=js,vue",
    "lint:fix": "eslint ./src --fix --ext=js,vue",
    "lint:safe": "eslint ./src --ext=js,vue ;exit 0",
    "build": "webpack --progress --hide-modules --config webpack.config.js",
    "serve": "webpack-dev-server --inline --hot --config webpack.config.js -d --open --history-api-fallback",
    "deploy": "npm run build;rm -Rf ./public_html;mkdir public_html;cp -r ./dist ./public_html/dist;gulp deploy",
    "test": "npm run lint",
    "test:safe": "npm run lint; exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/potato4d/vue-dev-template.git"
  },
  "author": "Potato4d",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/potato4d/vue-dev-template/issues"
  },
  "homepage": "https://github.com/potato4d/vue-dev-template#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "css-loader": "^0.25.0",
    "dotenv": "^2.0.0",
    "eslint": "^3.8.1",
    "eslint-plugin-vue": "^0.1.1",
    "gulp": "^3.9.1",
    "gulp-s3-upload": "^1.6.1",
    "svg-url-loader": "^1.1.0",
    "vue": "^2.0.3",
    "vue-hot-reload-api": "^2.0.6",
    "vue-loader": "^9.7.0",
    "vue-router": "^2.0.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "whatwg-fetch": "^1.0.0"
  }
}
